  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "db:init": "node src/utils/initDb.js",
    "db:clean": "node src/utils/cleanDb.js",
    "db:migrate": "node src/utils/migrate.js",
    "db:seed": "node src/utils/seedData.js",
